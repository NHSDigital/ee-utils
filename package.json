{
  "name": "@NHSDigital/ee-utils",
  "version": "0.0.65",
  "license": "MIT",
  "type": "module",
  "author": "jacobgill1 <jacob.gill1@nhs.net>",
  "repository": {
    "type": "git",
    "url": "https://github.com/NHSDigital/ee-utils.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/cjs/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "files": [
    "lib/"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/client-ssm": "^3.519.0",
    "@aws-sdk/middleware-retry": "^3.374.0",
    "@octokit/core": "^6.1.2",
    "@octokit/plugin-throttling": "^9.2.1",
    "@octokit/rest": "^20.0.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mongodb-memory-server": "^9.1.6",
    "mongoose": "^8.2.0",
    "octokit": "^3.1.2",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.136",
    "@types/uuid": "^9.0.8",
    "aws-sdk-client-mock": "^3.0.1",
    "aws-sdk-client-mock-jest": "^3.0.1",
    "esbuild": "^0.23.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "scripts": {
    "test": "jest",
    "clean": "rm -rf lib",
    "build": "yarn clean && yarn build:types && yarn build:package",
    "build:package": "yarn build:cjs && yarn build:esm && yarn build:clean-package",
    "build:cjs": "esbuild --platform=node --format=cjs --outdir=lib/cjs src/*.ts src/**/*.ts",
    "build:esm": "esbuild --platform=node --format=esm --outdir=lib/esm src/*.ts src/**/*.ts --tsconfig=tsconfig.esm.json",
    "build:types": "yarn build:types-cjs && yarn build:types-esm",
    "build:types-cjs": "yarn tsc --emitDeclarationOnly --declaration -p tsconfig.json",
    "build:types-esm": "yarn tsc --emitDeclarationOnly --declaration -p tsconfig.esm.json",
    "build:clean-package": "rm -rf lib/**/__tests__"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
