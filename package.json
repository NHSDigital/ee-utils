{
  "name": "@NHSDigital/ee-utils",
  "version": "0.0.51-dev",
  "license": "MIT",
  "type": "module",
  "author": "jacobgill1 <jacob.gill1@nhs.net>",
  "repository": {
    "type": "git",
    "url": "https://github.com/NHSDigital/ee-utils.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/cjs/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "files": [
    "lib/"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.383.0",
    "@aws-sdk/client-ssm": "^3.382.0",
    "@aws-sdk/middleware-retry": "^3.374.0",
    "@octokit/rest": "^20.0.1",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "mongodb-memory-server": "^9.0.1",
    "mongoose": "^8.0.0",
    "octokit": "^3.1.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.9",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-env": "^7.22.10",
    "aws-sdk-client-mock": "^3.0.0",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "jest": "^29.6.2"
  },
  "scripts": {
    "test": "jest",
    "build-cjs": "babel src --out-dir lib/cjs --keep-file-extension --copy-files --presets=@babel/preset-env",
    "build-esm": "babel src --out-dir lib/esm --keep-file-extension --copy-files",
    "build": "yarn build-cjs && yarn build-esm"
  }
}
